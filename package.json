{
  "name": "vscode-plugin-markdown-image-pack",
  "displayName": "Markdown Images File Pack",
  "description": "",
  "keywords": [
    "vscode",
    "plugin",
    "markdown",
    "image"
  ],
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.31.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.packMarkdownImages"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.packMarkdownImages",
        "title": "Pack Markdown Images"
      }
    ],
    "keybindings": [
      {
        "command": "extension.packMarkdownImages",
        "key": "ctrl+m+p",
        "mac": "cmd_m+p",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorFocus",
          "command": "extension.packMarkdownImages",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "tslint": "^5.12.1",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "@types/uuid": "^3.4.4",
    "core-js": "^2.6.5",
    "jimp": "^0.6.0",
    "uuid": "^3.3.2"
  }
}
